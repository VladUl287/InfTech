<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - InfTech</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/bootstrap-icons/font/bootstrap-icons.css" />
    <link rel="stylesheet" href="~/@(nameof(InfTech)).styles.css" />
    <link rel="stylesheet" href="~/lib/highlight/styles/vs.min.css">
</head>
<body class="main-container">
    <header>
        <h4>Explorer</h4>
    </header>
    <main role="main" class="h-100 overflow-hidden">
        @RenderBody()
    </main>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/highlight/highlight.min.js"></script>
    <script src="~/lib/highlight/languages/csharp.min.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
